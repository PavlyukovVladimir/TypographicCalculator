<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.PVV.ControllerPrimary">
   <children>
      <Label text="Главное меню" />
      <Button fx:id="ofsetButton" maxWidth="200.0" minWidth="200.0" onAction="#switchToOfset" prefWidth="200.0" text="Считать офсетную печать" />
      <Button fx:id="risoButton" maxWidth="200.0" minWidth="200.0" onAction="#switchToRiso" prefWidth="200.0" text="Считать печать ризографом" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
